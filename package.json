{
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "eslint": "^8",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.8.0",
    "html-minifier-terser": "^7.2.0",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "rollup-plugin-copy": "^3.5.0",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.15.0",
    "vite": "^5.3.1"
  },
  "dependencies": {
    "date-fns": "^3.6.0"
  },
  "scripts": {
    "build": "vite build && mv dist/src/*.html dist/",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "prettier --check src && eslint src",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
    "minify": "echo html css | xargs -n1 echo | xargs -i html-minifier-terser --collapse-whitespace --remove-comments --minify-css true --minify-js true --file-ext {} --input-dir dist --output-dir dist"
  },
  "type": "module"
}
